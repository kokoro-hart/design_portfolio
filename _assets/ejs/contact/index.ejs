<% let pageId='contact' ; %>

  <%- include('../common/_header', {pageId:pageId }) %>

    <% console.log(jsonData) %>
      <main class="l-lower-contact" data-barba="container" data-barba-namespace="contact">
        <div class="l-container">
          <div id="js-first-view">
          <div class="p-lower-first-view">
            <h2 class="c-page-heading">
              <span class="c-page-heading__en is-wrap">
                <span class="is-slide-up">
                  CONTACT
                </span>
              </span>
              <span class="c-page-heading__ja is-wrap">
                <span class="is-slide-up">
                  お問い合わせ
                </span>
              </span>
            </h2>
          </div>
          <div class="l-lower-contact__contents is-slide-up">
            <h3 class="formTitle u-visually-hidden">お問い合わせフォーム</h3>
            <form id="js-form" class="p-form" action="https://docs.google.com/forms/u/0/d/e/1FAIpQLSdWVihzITkGsBjGj6KJVVhGOgP-tTKr2I5zKUkF3J8DoT1cjA/formResponse" method="POST" target="hidden_iframe" onsubmit="submitted=true;" aria-labelledby="formTitle">
              <dl class="p-form__dl">
                <!--お名前-->
                <div class="p-form__row">
                  <dt class="p-form__dt">
                    <label class="p-form__label p-form__label--required" for="your-name">お名前</label>
                  </dt>
                  <dd class="p-form__dd">
                    <input class="p-form__input" id="your-name" type="text" placeholder="例） cocosyu太郎" name="entry.228557334" autocomplete="name" required aria-required="true">
                  </dd>
                </div>
                <!--/お名前-->
          
                <!--御社名-->
                <div class="p-form__row">
                  <dt class="p-form__dt">
                    <label class="p-form__label" for="your-company">御社名</label>
                  </dt>
                  <dd class="p-form__dd">
                    <input class="p-form__input" id="your-company" type="text" name="entry.1322745816" placeholder="例） 株式会社cocosyu" autocomplete="organization">
                  </dd>
                </div>
                <!--/御社名-->
          
                <!--メールアドレス-->
                <div class="p-form__row">
                  <dt class="p-form__dt">
                    <label class="p-form__label p-form__label--required" for="your-email">メールアドレス</label>
                  </dt>
                  <dd class="p-form__dd">
                    <input class="p-form__input" id="your-email" type="email" placeholder="例） example@mail.com" js-name="YPqjbf" name="entry.1785101720" autocomplete="email" required aria-required="true">
                  </dd>
                </div>
                <!--/メールアドレス-->
          
                <!--お問い合わせ内容-->
                <div class="p-form__row">
                  <dt class="p-form__dt">
                    <label class="p-form__label p-form__label--required" for="your-textarea">お問い合わせ内容</label>
                  </dt>
                  <dd class="p-form__dd">
                    <textarea class="p-form__input p-form__textarea maxlength" id="your-textarea" placeholder="お問い合わせ内容を入力してください。" name="entry.2130733584" required aria-required="true"></textarea>
                  </dd>
                </div>
                <!--/お問い合わせ内容-->
          
                <!--送信ボタン-->
                <div class="p-form__submit-wrapper">
                  <input id="submit" class="c-button-submit" type="submit" value="送信">
                </div>
                <!--送信ボタン-->
              </dl>
            </form>
          </div>
          </div>
        </div>
      </main>
  <%- include('../common/_footer', {pageId:pageId }) %>