<% let pageId='open-cafe' ; %>

  <%- include('../common/_header', {pageId:pageId }) %>

    <% console.log(jsonData) %>
      <main id="js-switching-area" class="l-single-works p-single-works p-single-works--open">
        <div class="l-container">
          <div id="js-switching-trigger">
            <!--ファーストビュー-->
            <div id="js-first-view" class="l-single-works__first-view">
              <div class="p-single-works__heading c-single-heading">
                <h1 class="p-single-works__heading-en is-wrap" style="transform: translate(0px, 0px);">
                  <span class="is-slide-up">
                    Open Cafe
                  </span>
                </h1>
                <p class="p-single-works__heading-ja is-fade-up">
                  架空カフェサイト
                </p>
                <p class="p-single-works__category is-fade-up">
                  Corporate Site / Coding
                </p>
              </div>
            </div>
            <!--/ファーストビュー-->

            <!--モックアップ-->
            <figure class="l-single-works__figure p-single-works__figure">
              <picture class="p-single-works__picture p-single-works__picture--sp" data-scroll data-scroll-speed="1"
                data-scroll-delay="0.1">
                <source srcset="/img/webp/sp-open.webp" class="p-single-works__img" type="image/webp" />
                <img src="/img/common/sp-open.png" class="p-single-works__img" alt="Open Cafe spモックアップ"
                  style="transform: translate(0px, 0px);">
              </picture>

              <picture class="p-single-works__picture p-single-works__picture--pc" data-scroll data-scroll-speed="0.5">
                <source srcset="/img/webp/pc-open.webp" class="p-single-works__img" type="image/webp" />
                <img src="/img/common/pc-open.png" class="p-single-works__img" alt="Open Cafe pcモックアップ"
                  style="transform: translate(0px, 0px);">
              </picture>

              <picture class="p-single-works__picture p-single-works__picture--tab" data-scroll data-scroll-speed="0.75"
                data-scroll-delay="0.05">
                <source srcset="/img/webp/tab-open.webp" class="p-single-works__img" type="image/webp" />
                <img src="/img/common/tab-open.png" class="p-single-works__img" alt="Open Cafe tabモックアップ"
                  style="transform: translate(0px, 0px);">
              </picture>
            </figure>
            <!--モックアップ-->
          </div>
        </div>

        <div class="l-container l-container--small">
          <div class="l-single-works__contents">
            <!--概要-->
            <div class="p-single-works__info" style="transform: translate(0px, 0px);">
              <section class="p-single-works__overview p-works-overview">
                <h2 class="p-works-overview__heading is-wrap js-trigger">
                  <span class="is-slide-up">
                    LINKS
                  </span>
                </h2>
                <div class="p-works-overview__body">
                  <dl class="p-works-overview__dl">
                    <div class="p-works-overview__row">
                      <dt class="p-works-overview__dt">
                        Site url
                      </dt>
                      <dd class="p-works-overview__dd">
                        <a href="" class="p-works-overview__link" target="_blank" rel="noopener noreferrer"
                          style="transform: translate(0px, 0px);">
                          https://open-cafe.com/
                          <svg class="c-svg p-works-overview__blank" width="14" height="14">
                            <use xlink:href="/img/svg/sprite.min.svg#icon-blank" />
                          </svg>
                        </a>
                      </dd>
                    </div>
                    <div class="p-works-overview__row">
                      <dt class="p-works-overview__dt">
                        Source
                        <svg class="c-svg p-works-overview__svg" width="20" height="20" role="img"
                          aria-label="GitHubのアイコン">
                          <use xlink:href="/img/svg/sprite.min.svg#icon-github" />
                        </svg>
                      </dt>
                      <dd class="p-works-overview__dd">
                        <a href="https://github.com/kokoro-hart/global_standard" class="p-works-overview__link"
                          target="_blank" rel="noopener noreferrer" style="transform: translate(0px, 0px);">
                          https://github.com/kokoro-hart
                          <svg class="c-svg p-works-overview__blank" width="14" height="14">
                            <use xlink:href="/img/svg/sprite.min.svg#icon-blank" />
                          </svg>
                        </a>
                      </dd>
                    </div>
                    <div class="p-works-overview__row">
                      <dt class="p-works-overview__dt">
                        Basic認証
                      </dt>
                      <dd class="p-works-overview__dd">
                        ID : open <br>
                        Pass : open
                      </dd>
                    </div>
                  </dl>
                </div>
              </section>
              <section class="p-single-works__overview p-works-overview">
                <h2 class="p-works-overview__heading is-wrap js-trigger">
                  <span class="is-slide-up">
                    OVERVIEW
                  </span>
                </h2>
                <div class="p-works-overview__body">
                  <p class="p-works-overview__text">
                    最高のコーヒーと、 時の流れを味わうことができる「Open Cafe - dish & coffee」架空カフェサイトのコーディング、WordPress化を行いました。
                  </p>
                </div>
              </section>
            </div>
            <!--/概要-->

            <!--斜めのモックアップ-->
            <picture class="p-single-works__rotate-picture" style="transform: translate(0px, 0px);">
              <source srcset="/img/webp/open-rotate.webp" class="p-single-works__rotate-img" type="image/webp" />
              <img src="/img/common/open-rotate.jpeg" class="p-single-works__rotate-img" width="1920" height="1331.81" loading="lazy" decoding="async" data-scroll data-scroll-speed="-1" alt="斜めのモックアップ画像">
            </picture>
            <picture class="p-single-works__rotate-picture" style="transform: translate(0px, 0px);">
              <source srcset="/img/webp/open-rotate.webp" class="p-single-works__rotate-img" type="image/webp" />
              <img src="/img/common/open-rotate.jpeg" class="p-single-works__rotate-img" width="1920" height="1331.81" loading="lazy" decoding="async" data-scroll data-scroll-speed="-1" alt="斜めのモックアップ画像">
            </picture>
            <!--/斜めのモックアップ-->

            <!--詳細-->
            <section class="p-single-works__detail p-works-detail" data-scroll data-scroll-sticky
              data-scroll-target="#js-capture">
              <div class="p-works-detail__inner">
                <h2 class="js-trigger" style="transform: translate(0px, 0px);">
                  <span class="p-works-detail__heading">
                    ROLE
                  </span>
                </h2>
                <div class="p-works-detail__body">
                  <dl class="p-works-detail__dl">
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        担当範囲
                      </dt>
                      <dd class="p-works-detail__dd">
                        コーディング
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        制作期間
                      </dt>
                      <dd class="p-works-detail__dd">
                        3週間 (2〜3h / day)
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        使用言語
                      </dt>
                      <dd class="p-works-detail__dd">
                        HTML(EJS) / CSS(Scss) / JavaScript / PHP
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        使用ツール
                      </dt>
                      <dd class="p-works-detail__dd">
                        Git / Gulp / vscode / XD
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        使用CMS
                      </dt>
                      <dd class="p-works-detail__dd">
                        WordPress
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        工夫点
                      </dt>
                      <dd class="p-works-detail__dd">
                        世界で活躍できるグローバルな人材を育てる、架空事業会社のコーポレートサイトのコーディング、WordPress化を行いました。
                      </dd>
                    </div>
                  </dl>
                </div>
              </div>
            </section>
            <!--/詳細-->

            <!--キャプチャ-->
            <div class="p-single-works__capture">
              <picture class="p-single-works__capture-picture">
                <source srcset="/img/webp/open-top.webp" class="p-single-works__capture-img" type="image/webp" />
                <img src="/img/common/open-top.jpeg" class="p-single-works__capture-img" alt="作品のタイトル" width="816" height="3586.23" loading="lazy" decoding="async" >
              </picture>
              <picture class="p-single-works__capture-picture">
                <source srcset="/img/webp/open-concept.webp" class="p-single-works__capture-img" type="image/webp" />
                <img src="/img/common/open-concept.jpeg" class="p-single-works__capture-img" alt="作品のタイトル" width="816" height="950.09" loading="lazy" decoding="async" >
              </picture>

              <picture class="p-single-works__capture-picture">
                <source srcset="/img/webp/open-menu.webp" class="p-single-works__capture-img" type="image/webp" />
                <img src="/img/common/open-menu.jpeg" class="p-single-works__capture-img" alt="作品のタイトル" width="816" height="780.66" loading="lazy" decoding="async" >
              </picture>

              <picture class="p-single-works__capture-picture">
                <source srcset="/img/webp/open-gift.webp" class="p-single-works__capture-img" type="image/webp" />
                <img src="/img/common/open-gift.jpeg" class="p-single-works__capture-img" alt="作品のタイトル" width="816" height="752.06" loading="lazy" decoding="async" >
              </picture>
              <div class="p-single-works__capture-mask" id="js-capture"></div>
            </div>
            <!--/キャプチャ-->
          </div>
        </div>

        <div class="p-works-connect" style="transform: translate(0px, 0px);">
          <div class="p-works-connect__inner">
            <a href="/global-standard/" class="p-works-connect__link">
              <span class="p-works-connect__link-head">
                Prev
              </span>
              <span class="p-works-connect__link-foot">
                Global Standard
              </span>
            </a>
          </div>
          <div class="p-works-connect__inner">
            <a href="/minami-dental/" class="p-works-connect__link">
              <span class="p-works-connect__link-head">
                Next
              </span>
              <span class="p-works-connect__link-foot">
                Minami Dental
              </span>
            </a>
          </div>
        </div>

      </main>

      <%- include('../common/_footer', {pageId:pageId }) %>