<% let pageId='minami-dental' ; %>

<%- include('../common/_header', {pageId:pageId }) %>

<% console.log(jsonData) %>
<main id="js-minami-dental" class="l-single-works p-single-works" data-barba="container" data-barba-namespace="single" data-bg="#8DC7E3">
  <div class="l-container l-container--small">
    <%# 背景色変更エリア %>
    <div id="js-switching-trigger">
      <%# ファーストビュー %>
      <div id="js-first-view" class="l-single-works__first-view">
        <div class="p-single-works__heading c-single-heading">
          <h1 class="p-single-works__heading-en is-wrap" style="transform: translate(0px, 0px);">
            <span class="is-slide-up">
              Minami Dental
            </span>
          </h1>
          <p class="p-single-works__heading-ja is-fade-up u-delay01">
            架空歯科クリニックサイト
          </p>
          <p class="p-single-works__category is-fade-up u-delay02">
            Dental Site / Coding
          </p>
        </div>
      </div><%# / ファーストビュー %>

      <%# モックアップ %>
      <div class="p-single-works__mockups" style="transform: translate(0px, 0px);">
        <picture class="p-single-works__mockup p-single-works__mockup--sp" data-scroll data-scroll-speed="1" data-scroll-delay="0.1">
          <source srcset="/img/webp/sp-minami.webp" class="p-single-works__mockup-img" type="image/webp" />
          <img src="/img/common/sp-minami.png" class="p-single-works__mockup-img" alt="みなみ歯科クリニック架空サイト、スマホ画面のビュー" width="70" height="141" decoding="async">
        </picture>

        <picture class="p-single-works__mockup p-single-works__mockup--pc" data-scroll data-scroll-speed="0.5">
          <source srcset="/img/webp/pc-minami.webp" class="p-single-works__mockup-img" type="image/webp" />
          <img src="/img/common/pc-minami.png" class="p-single-works__mockup-img" alt="みなみ歯科クリニック架空サイト、PC画面のビュー" width="949" height="811" decoding="async">
        </picture>

        <picture class="p-single-works__mockup p-single-works__mockup--tab" data-scroll data-scroll-speed="0.75" data-scroll-delay="0.05">
          <source srcset="/img/webp/tab-minami.webp" class="p-single-works__mockup-img" type="image/webp" />
          <img src="/img/common/tab-minami.png" class="p-single-works__mockup-img" alt="みなみ歯科クリニック架空サイト、タブレット画面のビュー" width="445" height="622" decoding="async">
        </picture>
      </div><%# / モックアップ %>

      <%# サイトリンク 概要 %>
      <div class="p-single-works__info" style="transform: translate(0px, 0px);">
        <section class="p-single-works__overview p-works-overview">
          <h2 class="p-works-overview__heading">
            LINKS
          </h2>
          <div class="p-works-overview__body">
            <dl class="p-works-overview__dl">
              <div class="p-works-overview__row">
                <dt class="p-works-overview__dt">
                  Site url
                </dt>
                <dd class="p-works-overview__dd">
                  <a href="http://pf-minamidental.com/" class="p-works-overview__link" target="_blank" rel="noopener noreferrer"
                    style="transform: translate(0px, 0px);">
                    http://pf-minamidental.com/
                    <svg class="c-svg p-works-overview__blank" width="14" height="14">
                      <use xlink:href="/img/svg/sprite.min.svg#icon-blank" />
                    </svg>
                  </a>
                </dd>
              </div>
              <div class="p-works-overview__row">
                <dt class="p-works-overview__dt">
                  Source
                  <svg class="c-svg p-works-overview__svg" width="20" height="20" role="img" aria-label="GitHubのアイコン">
                    <use xlink:href="/img/svg/sprite.min.svg#icon-github" />
                  </svg>
                </dt>
                <dd class="p-works-overview__dd">
                  <a href="https://github.com/kokoro-hart/minato_dental" class="p-works-overview__link"
                    target="_blank" rel="noopener noreferrer" style="transform: translate(0px, 0px);">
                    https://github.com/kokoro-hart
                    <svg class="c-svg p-works-overview__blank" width="14" height="14">
                      <use xlink:href="/img/svg/sprite.min.svg#icon-blank" />
                    </svg>
                  </a>
                </dd>
              </div>
              <div class="p-works-overview__row">
                <dt class="p-works-overview__dt">
                  Basic認証
                </dt>
                <dd class="p-works-overview__dd">
                  ID : pf-minami <br>
                  Pass : pf-minami@01
                </dd>
              </div>
            </dl>
          </div>
        </section>
        <section class="p-single-works__overview p-works-overview">
          <h2 class="p-works-overview__heading">
            OVERVIEW
          </h2>
          <div class="p-works-overview__body">
            <p class="p-works-overview__text">
              「みなみ歯科クリニック」架空歯科クリニックサイトのコーディング、CMS構築を行いました。
            </p>
          </div>
        </section>
      </div><%# / サイトリンク 概要 %>

      <%# ワイドイメージ %>
      <picture class="p-single-works__picture p-single-works__picture--wide" style="transform: translate(0px, 0px);">
        <source srcset="/img/webp/minami-rotate.webp" class="p-single-works__img" type="image/webp" />
        <img src="/img/common/minami-rotate.png" class="p-single-works__img" width="1920" height="1333" loading="lazy" data-scroll data-scroll-speed="-1" alt="">
      </picture><%# / ワイドイメージ %>

      <%# フューチャー %>
      <div class="p-single-works__info" style="transform: translate(0px, 0px);">
        <section class="p-single-works__overview p-works-overview">
          <h2 class="p-works-overview__heading">
            FEATURE
          </h2>
          <div class="p-works-overview__body">
            <p class="p-works-overview__text">
              WordPress自作テーマにてブログ、お知らせ、スタッフ紹介、診察案内を管理しています。
            </p>
          </div>
        </section>
      </div><%# / フューチャー %>

      <%# 動画 %>
      <div class="p-single-works__video-wrapper">
        <video autoplay muted loop playsinline width="1632" height="1020" data-scroll data-scroll-speed="-1">
          <source src="/img/video/minami-video.mp4" type="video/mp4">
        </video>
      </div><%# / 動画 %>

    </div><%# / 背景色変更エリア %>

    <%# 詳細 %>
    <div class="l-single-works__detail">
      <section class="p-single-works__detail p-works-detail" data-scroll data-scroll-sticky data-scroll-target="#js-capture">
        <div class="p-works-detail__inner">
          <h2 class="p-works-detail__heading" style="transform: translate(0px, 0px);">
            DETAILS
          </h2>
          <div class="p-works-detail__body">
            <dl class="p-works-detail__dl">
              <div class="p-works-detail__row">
                <dt class="p-works-detail__dt">
                  担当範囲
                </dt>
                <dd class="p-works-detail__dd">
                  コーディング（12P） / CMS構築
                </dd>
              </div>
              <div class="p-works-detail__row">
                <dt class="p-works-detail__dt">
                  制作期間
                </dt>
                <dd class="p-works-detail__dd">
                  2021.10.20 - 2021.12.19
                </dd>
              </div>
              <div class="p-works-detail__row">
                <dt class="p-works-detail__dt">
                  実働時間
                </dt>
                <dd class="p-works-detail__dd">
                  30日(2〜4時間/ 1日)
                </dd>
              </div>
              <div class="p-works-detail__row">
                <dt class="p-works-detail__dt">
                  使用言語
                </dt>
                <dd class="p-works-detail__dd">
                  HTML(EJS) / CSS(Scss) / JavaScript / PHP
                </dd>
              </div>
              <div class="p-works-detail__row">
                <dt class="p-works-detail__dt">
                  使用ツール
                </dt>
                <dd class="p-works-detail__dd">
                  git / gulp + webpack / vscode / XD
                </dd>
              </div>
              <div class="p-works-detail__row">
                <dt class="p-works-detail__dt">
                  ライブラリ
                </dt>
                <dd class="p-works-detail__dd">
                  swiper.js
                </dd>
              </div>
              <div class="p-works-detail__row">
                <dt class="p-works-detail__dt">
                  使用CMS
                </dt>
                <dd class="p-works-detail__dd">
                  WordPress
                </dd>
              </div>
            </dl>
          </div>
        </div>
      </section>
      <div class="p-single-works__captures">
        <picture class="p-single-works__capture" style="transform: translate(0px, 0px);">
          <source srcset="/img/webp/minami-top.webp" class="p-single-works__capture-img" type="image/webp" />
          <img src="/img/common/minami-top.png" class="p-single-works__capture-img" alt="みなみ歯科クリニック架空サイトのトップページフルスクリーンキャプチャ" width="816" height="1932" loading="lazy">
        </picture>
        <picture class="p-single-works__capture" style="transform: translate(0px, 0px);">
          <source srcset="/img/webp/minami-about.webp" class="p-single-works__capture-img" type="image/webp" />
          <img src="/img/common/minami-about.png" class="p-single-works__capture-img" alt="みなみ歯科クリニック架空サイトのカンパニーページフルスクリーンキャプチャ" width="816" height="1046" loading="lazy">
        </picture>
        <picture class="p-single-works__capture" style="transform: translate(0px, 0px);">
          <source srcset="/img/webp/minami-staff.webp" class="p-single-works__capture-img" type="image/webp" />
          <img src="/img/common/minami-staff.png" class="p-single-works__capture-img" alt="みなみ歯科クリニック架空サイトのサービスページフルスクリーンキャプチャ" width="816" height="1376" loading="lazy">
        </picture>
        <div class="p-single-works__capture-mask" id="js-capture"></div>
      </div>
    </div><%# / 詳細 %>
  </div>

  <%# 制作実績導線 %>
  <div class="p-works-connect" style="transform: translate(0px, 0px);">
    <a href="/global-standard/" class="p-works-connect__link">
      <picture class="p-works-connect__bg">
        <source srcset="/img/webp/thumbnail-global.webp" type="image/webp" />
        <img src="/img/common/thumbnail-global.jpeg" width="770" height="781" alt="サムネイル、Global Standard架空コーポレートサイト" loading="lazy">
      </picture>
      <p class="p-works-connect__text-wrapper">
        <span class="p-works-connect__text" data-text="Global Standard">
          Prev
        </span>
      </p>
    </a>
    <a href="/garden-salad/" class="p-works-connect__link">
      <picture class="p-works-connect__bg">
        <source srcset="/img/webp/thumbnail-garden.webp" type="image/webp" />
        <img src="/img/common/thumbnail-garden.jpeg" width="770" height="781" alt="サムネイル、Garden Salad架空カフェサイト" loading="lazy">
      </picture>
      <p class="p-works-connect__text-wrapper">
        <span class="p-works-connect__text" data-text="Garden Salad">
          Next
        </span>
      </p>
    </a>
  </div><%# / 制作実績導線 %>

</main>

<%- include('../common/_footer', {pageId:pageId }) %>