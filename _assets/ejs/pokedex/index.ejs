<% let pageId='pokedex' ; %>

  <%- include('../common/_header', {pageId:pageId }) %>

    <% console.log(jsonData) %>
      <main id="js-pokedex-app" class="l-single-works p-single-works" data-barba="container"
        data-barba-namespace="single" data-bg="#AAAADA">
        <div class="l-container l-container--small">
          <%# 背景色変更エリア %>
          <div id="js-switching-trigger">
            <%# ファーストビュー %>
            <div id="js-first-view" class="l-single-works__first-view">
              <div class="p-single-works__heading c-single-heading">
                <h1 class="p-single-works__heading-en is-wrap" style="transform: translate(0px, 0px);">
                  <span class="is-slide-up">
                    PokeDex
                  </span>
                </h1>
                <p class="p-single-works__heading-ja is-wrap">
                  <span class="is-slide-up u-delay04">
                    Gallery Site
                  </span>
                </p>
                <p class="p-single-works__category is-wrap">
                  <span class="is-slide-up u-delay05">
                    Some UI design / Development
                  </span>
                </p>
              </div>
            </div>
            <%# / ファーストビュー %>

            <%# モックアップ %>
            <div class="p-single-works__mockups" style="transform: translate(0px, 0px);">
              <picture class="p-single-works__mockup p-single-works__mockup--sp" data-scroll
                data-scroll-speed="1" data-scroll-delay="0.1">
                <!-- <source srcset="/img/webp/sp-brain.webp" class="p-single-works__mockup-img" type="image/webp" /> -->
                <img src="/img/common/sp-pokedex.png" class="p-single-works__mockup-img"
                  alt="pokedexポケモンWeb図鑑、スマホ画面のビュー" width="70" height="141" decoding="async">
              </picture>

              <picture class="p-single-works__mockup p-single-works__mockup--pc" data-scroll
                data-scroll-speed="0.5">
                <!-- <source srcset="/img/webp/pc-brain.webp" class="p-single-works__mockup-img" type="image/webp" /> -->
                <img src="/img/common/pc-pokedex.png" class="p-single-works__mockup-img"
                  alt="pokedexポケモンWeb図鑑、PC画面のビュー" width="949" height="811" decoding="async">
              </picture>

              <picture class="p-single-works__mockup p-single-works__mockup--tab" data-scroll
                data-scroll-speed="0.75" data-scroll-delay="0.05">
                <!-- <source srcset="/img/webp/tab-brain.webp" class="p-single-works__mockup-img"type="image/webp" /> -->
                <img src="/img/common/tab-pokedex.png" class="p-single-works__mockup-img"
                  alt="pokedexポケモンWeb図鑑、タブレット画面のビュー" width="445" height="622" decoding="async">
              </picture>
            </div>
            <%# / モックアップ %>

            <%# サイトリンク 概要 %>
            <div class="p-single-works__info" style="transform: translate(0px, 0px);">
              <section class="p-single-works__overview p-works-overview">
                <h2 class="p-works-overview__heading">
                  LINKS
                </h2>
                <div class="p-works-overview__body">
                  <dl class="p-works-overview__dl">
                    <div class="p-works-overview__row">
                      <dt class="p-works-overview__dt">
                        Site url
                      </dt>
                      <dd class="p-works-overview__dd">
                        <a href="https://pokedex-six-mocha.vercel.app/" class="p-works-overview__link"
                          target="_blank" rel="noopener noreferrer" style="transform: translate(0px, 0px);">
                          https://pokedex-six-mocha.vercel.app/
                          <svg class="c-svg p-works-overview__blank" width="14" height="14">
                            <use xlink:href="/img/svg/sprite.min.svg#icon-blank" />
                          </svg>
                        </a>
                      </dd>
                    </div>
                  </dl>
                </div>
              </section>
              <section class="p-single-works__overview p-works-overview">
                <h2 class="p-works-overview__heading">
                  OVERVIEW
                </h2>
                <div class="p-works-overview__body">
                  <p class="p-works-overview__text">
                    PokeAPIを利用してポケモンWeb図鑑を制作しました。ガラル地方のポケモンNo.897まで掲載しています。
                  </p>
                </div>
              </section>
            </div>
            <%# / サイトリンク 概要 %>

            <%# ワイドイメージ %>
            <picture class="p-single-works__picture p-single-works__picture--wide"
              style="transform: translate(0px, 0px);">
              <source srcset="/img/webp/pokedex-rotate01.webp" class="p-single-works__img" type="image/webp" />
              <img src="/img/common/pokedex-rotate01.png" class="p-single-works__img" width="1920"
                height="1333" loading="lazy" data-scroll data-scroll-speed="-1" alt="">
            </picture>
            <picture class="p-single-works__picture p-single-works__picture--wide"
              style="transform: translate(0px, 0px);">
              <source srcset="/img/webp/pokedex-rotate02.webp" class="p-single-works__img" type="image/webp" />
              <img src="/img/common/pokedex-rotate02.png" class="p-single-works__img" width="1920"
                height="1333" loading="lazy" data-scroll data-scroll-speed="-1" alt="">
            </picture>
            <picture class="p-single-works__picture p-single-works__picture--wide"
              style="transform: translate(0px, 0px);">
              <source srcset="/img/webp/pokedex-rotate03.webp" class="p-single-works__img" type="image/webp" />
              <img src="/img/common/pokedex-rotate03.png" class="p-single-works__img" width="1920"
                height="1333" loading="lazy" data-scroll data-scroll-speed="-1" alt="">
            </picture>
            <picture class="p-single-works__picture p-single-works__picture--wide"
              style="transform: translate(0px, 0px);">
              <source srcset="/img/webp/pokedex-rotate04.webp" class="p-single-works__img" type="image/webp" />
              <img src="/img/common/pokedex-rotate04.png" class="p-single-works__img" width="1920"
                height="1333" loading="lazy" data-scroll data-scroll-speed="-1" alt="">
            </picture>
            <%# ワイドイメージ %>

            <%# 動画 %>
            <div class="p-single-works__video-wrapper">
              <video autoplay muted loop playsinline width="1351" height="844" data-scroll
                data-scroll-speed="-1">
                <source src="/img/video/pokedex-video.mp4" type="video/mp4">
              </video>
            </div>
            <%# / 動画 %>

          </div>
          <%# / 背景色変更エリア %>

          <%# 詳細 %>
          <div class="l-single-works__detail">
            <section class="p-single-works__detail p-works-detail" data-scroll data-scroll-sticky
              data-scroll-target="#js-capture">
              <div class="p-works-detail__inner">
                <h2 class="p-works-detail__heading" style="transform: translate(0px, 0px);">
                  DETAILS
                </h2>
                <div class="p-works-detail__body">
                  <dl class="p-works-detail__dl">
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        担当範囲
                      </dt>
                      <dd class="p-works-detail__dd">
                        デザイン / コーディング
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        制作期間
                      </dt>
                      <dd class="p-works-detail__dd">
                        2022.12.4 - 2022.12.7
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        実働時間
                      </dt>
                      <dd class="p-works-detail__dd">
                        3日 (1〜2時間/ 1日)
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        使用技術
                      </dt>
                      <dd class="p-works-detail__dd">
                        HTML / CSS / JavaScript
                      </dd>
                    </div>
                    <div class="p-works-detail__row">
                      <dt class="p-works-detail__dt">
                        使用ツール
                      </dt>
                      <dd class="p-works-detail__dd">
                        vscode / Figma
                      </dd>
                    </div>
                  </dl>
                </div>
              </div>
            </section>
            <div class="p-single-works__captures">
              <picture class="p-single-works__capture" style="transform: translate(0px, 0px);">
                <source srcset="/img/webp/pokemon.webp" class="p-single-works__capture-img" type="image/webp" />
                <img src="/img/common/pokemon.png" class="p-single-works__capture-img"
                  alt="" width="1079" height="3714" loading="lazy">
              </picture>
              <div class="p-single-works__capture-mask" id="js-capture"></div>
            </div>
          </div>
          <%# / 詳細 %>
        </div>

        <%# 制作実績導線 %>
          <div class="p-works-connect" style="transform: translate(0px, 0px);">
            <a href="/garden-salad/" class="p-works-connect__link">
              <picture class="p-works-connect__bg">
                <source srcset="/img/webp/thumbnail-garden.webp" type="image/webp" />
                <img src="/img/common/thumbnail-garden.jpeg" width="770" height="781" alt="サムネイル、Garden Salad架空カフェサイト"
                  loading="lazy">
              </picture>
              <p class="p-works-connect__text-wrapper">
                <span class="p-works-connect__text" data-text="Garden Salad">
                  Prev
                </span>
              </p>
            </a>
            <a href="/brain-app/" class="p-works-connect__link">
              <picture class="p-works-connect__bg">
                <source srcset="/img/webp/thumbnail-brain.webp" type="image/webp" />
                <img src="/img/common/thumbnail-brain.png" width="770" height="781" alt="サムネイル、Brain App架空医療系アプリLP" loading="lazy">
              </picture>
              <p class="p-works-connect__text-wrapper">
                <span class="p-works-connect__text" data-text="Brain App">
                  Next
                </span>
              </p>
            </a>
          </div>
          <%# / 制作実績導線 %>
      </main>

      <%- include('../common/_footer', {pageId:pageId }) %>